{
  "name": "seadanceai-transcript-sdk",
  "version": "0.1.4",
  "description": "Seadance AI SDK for extracting YouTube transcripts, languages, and AI-ready summaries.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "seadance",
    "ai",
    "youtube",
    "transcript",
    "summary",
    "sdk"
  ],
  "homepage": "https://seadanceai.com",
  "bugs": {
    "url": "https://seadanceai.com/contact"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeikia/seadanceai-transcript-sdk.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest run"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "youtube-transcript-js-api": "^1.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.17.24",
    "eslint": "^9.11.1",
    "tsup": "^8.3.0",
    "typescript": "^5.8.2",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
